<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <!--
        The bean for performing native configuration.
    -->
    <bean id="nativeConfigurator"
          class="org.lastbamboo.client.nativeos.NativeConfiguratorProxy">
        </bean>

    <!--
        The bean for launching file dialogs.
    -->
    <bean id="fileDialogLauncher"
          class="org.lastbamboo.client.nativeos.FileDialogLauncherProxy">
        </bean>

    </beans>
